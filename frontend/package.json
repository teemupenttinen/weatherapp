    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.1.3",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
